{
  "name": "template",
  "version": "0.0.1",
  "versionDescription": "第一个版本",
  "description": "template_description",
  "private": true,
  "dependencies": {
    "@pefish/js-helper-logger": "^0.0.8",
    "@pefish/js-node-assist": "^0.6.11",
    "@pefish/js-util-assert": "^0.0.3",
    "@pefish/js-util-config": "^0.2.3",
    "@pefish/js-util-desensitize": "^0.1.0",
    "@pefish/js-util-file": "^0.0.4",
    "@pefish/js-util-httprequest": "^0.1.4",
    "@pefish/js-util-time": "^0.1.1",
    "tree-kill": "^1.2.2",
    "cross-spawn": "^7.0.3"
  },
  "productName": "template",
  "main": "lib/start.js",
  "scripts": {
    "build-server": "rimraf -rf lib && tsc && uglifyjs-folder lib -eo lib -x .js",
    "build-client": "cd client && yarn build && cd ../",
    "start": "cross-env NODE_CONFIG=`pwd`/config/local.yaml node scripts/start.js",
    "start-no-client": "cross-env NO_CLIENT=1 yarn start",
    "start-client": "cd client && yarn start",
    "start-prod": "yarn build && electron .",
    "build": "yarn build-client && yarn build-server",
    "release": "node scripts/release.js"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "electron": "^8.5.2",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.10.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "uglifyjs-folder": "^1.5.1",
