{
  "name": "template",
  "version": "0.0.1",
  "description": "template_description",
  "private": true,
  "dependencies": {
    "@pefish/js-electron-common": "^0.3.10",
    "@pefish/js-helper-logger": "^0.0.8",
    "@pefish/js-node-assist": "^0.6.11",
    "@pefish/js-util-assert": "^0.0.3",
    "@pefish/js-util-config": "^0.2.3",
    "@pefish/js-util-file": "^0.0.4"
  },
  "productName": "template",
  "main": "lib/start.js",
  "scripts": {
    "build": "rimraf -rf lib && tsc",
    "start": "yarn build && NODE_CONFIG=`pwd`/config/local.yaml electron .",
    "start-prod": "yarn build && electron .",
    "pack-mac": "electron-rebuild -a x64 && electron-packager ./ template --platform=mas --arch=x64 --electron-version=5.0.2 --out=../out --overwrite --ignore=\"(.git|yarn.lock|tsconfig.json)\" --package-manager=yarn --icon=icon.icns",
    "pack-win": "electron-rebuild -a x64 && electron-packager ./ template --platform=win32 --arch=x64 --electron-version=5.0.2 --out=../out --version-string.CompanyName=PEFISH --overwrite --ignore=\"(.git|yarn.lock|tsconfig.json)\" --package-manager=yarn --icon=icon.ico"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "across-env": "^1.0.8",
    "electron": "^5.0.2",
    "electron-packager": "^13.0.1",
    "electron-rebuild": "^1.8.5",
    "nodemon": "^1.18.7",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
