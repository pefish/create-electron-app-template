{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@pefish/js-electron-common": "^0.1.17",
    "@pefish/js-helper-logger": "^0.0.7",
    "@pefish/js-node-assist": "^0.6.6",
    "@pefish/js-util-assert": "^0.0.3",
    "@pefish/js-util-config": "^0.0.2",
    "@pefish/js-util-file": "^0.0.4"
  },
  "productName": "template",
  "main": "lib/start.js",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "start": "yarn build && NODE_CONFIG=`pwd`/config/local.json electron .",
    "start-prod": "yarn build && NODE_CONFIG=`pwd`/config/prod.json electron .",
    "pack-mac": "electron-packager ./ template --platform=mas --arch=x64 --electron-version=5.0.2 --out=../out --overwrite --ignore=yarn.lock --package-manager=yarn --icon=icon.icns",
    "pack-mac-all": "yarn build && yarn build-server && yarn pack-mac"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "electron": "^5.0.2",
    "electron-packager": "^13.0.1",
    "electron-rebuild": "^1.8.5",
    "nodemon": "^1.18.7",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}
