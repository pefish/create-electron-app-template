{
  "name": "template",
  "version": "0.0.1",
  "description": "template_description",
  "private": true,
  "dependencies": {
    "@pefish/js-electron-common": "^0.4.2",
    "@pefish/js-helper-logger": "^0.0.8",
    "@pefish/js-node-assist": "^0.6.11",
    "@pefish/js-util-assert": "^0.0.3",
    "@pefish/js-util-config": "^0.2.3",
    "@pefish/js-util-file": "^0.0.4"
  },
  "productName": "template",
  "main": "out/start.js",
  "scripts": {
    "build": "rimraf -rf lib out && tsc && yarn minify && rimraf -rf lib",
    "start": "yarn build && NODE_CONFIG=`pwd`/config/local.yaml electron .",
    "start-prod": "yarn build && electron .",
    "rebuild": "electron-rebuild -a x64",
    "minify": "uglifyjs-folder lib -eo out -x .js",
    "pack-mac": "yarn rebuild && npm_config_electron_mirror=https://npm.taobao.org/mirrors/electron/ npm_config_electron_custom_dir=8.0.0 DEBUG=* electron-packager ./ --platform=mas --arch=x64 --electron-version=8.0.0 --out=../out --overwrite --ignore=\"(.git|yarn.lock|tsconfig.json)\" --ignore=^/src --ignore=^/lib --icon=icon.icns",
    "pack-win": "yarn rebuild && npm_config_electron_mirror=https://npm.taobao.org/mirrors/electron/ npm_config_electron_custom_dir=8.0.0 DEBUG=* electron-packager ./ --platform=win32 --arch=x64 --electron-version=8.0.0 --out=../out --version-string.CompanyName=PEFISH --overwrite --ignore=\"(.git|yarn.lock|tsconfig.json)\" --ignore=^/src --ignore=^/lib --icon=icon.ico"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "across-env": "^1.0.8",
    "electron": "^8.0.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.10.0",
    "nodemon": "^1.18.7",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "uglifyjs-folder": "^1.5.1"
  }
}
